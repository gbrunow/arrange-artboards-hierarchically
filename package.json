{
  "name": "tidy-up",
  "repository": {
    "url": "https://github.com/gbrunow/arrange-artboards-hierarchically"
  },
  "description": "Arrange artboards hierarchically depending on their name (based on the Sketch Slash naming pattern).",
  "version": "1.0.0-alpha.1",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Tidy Up",
    "manifest": "src/manifest.json",
    "main": "tidy-up.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.8.0"
  },
  "author": "Gui Brunow Gomes <gbrunow@outlook.com>"
}
