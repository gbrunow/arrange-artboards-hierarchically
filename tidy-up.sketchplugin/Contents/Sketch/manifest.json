{
  "$schema": "https://raw.githubusercontent.com/sketch-hq/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "commands": [
    {
      "handlers": {
        "run": "tidyUpCurrent"
      },
      "identifier": "tidy-up.this-page",
      "name": "Tidy Up This Page",
      "script": "__main.js",
      "shortcut": "control command t"
    },
    {
      "handlers": {
        "run": "tidyUpAll"
      },
      "identifier": "tidy-up.all",
      "name": "Tidy Up All Pages",
      "script": "__main.js",
      "shortcut": "ctrl t"
    },
    {
      "handlers": {
        "run": "arrangeCurrent"
      },
      "identifier": "tidy-up.arrange-page-artboards",
      "name": "Arrange Page Artboards",
      "script": "__main.js",
      "shortcut": "control command a"
    },
    {
      "handlers": {
        "run": "arrangeAll"
      },
      "identifier": "tidy-up.arrange-all-artboards",
      "name": "Arrange All Artboards",
      "script": "__main.js",
      "shortcut": "control 1"
    },
    {
      "handlers": {
        "run": "sortCurrent"
      },
      "identifier": "tidy-up.sort-page",
      "name": "Sort Page Artboards",
      "script": "__main.js",
      "shortcut": "control command o"
    },
    {
      "handlers": {
        "run": "sortAll"
      },
      "identifier": "tidy-up.sort-all",
      "name": "Sort All Artboards",
      "script": "__main.js",
      "shortcut": "control o"
    }
  ],
  "icon": "icon.png",
  "menu": {
    "items": [
      "tidy-up.all",
      "tidy-up.this-page",
      "tidy-up.arrange-all-artboards",
      "tidy-up.arrange-page-artboards",
      "tidy-up.sort-all",
      "tidy-up.sort-page"
    ],
    "title": "Tidy Up"
  },
  "version": "2.0.0-alpha.1",
  "description": "Arrange artboards hierarchically depending on their name (based on the Sketch Slash naming pattern).",
  "name": "Tidy Up",
  "identifier": "Tidy Up",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/gbrunow/arrange-artboards-hierarchically/master/.appcast.xml",
  "author": "Gui Brunow Gomes",
  "authorEmail": "gbrunow@outlook.com"
}